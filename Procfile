web: echo "Starting deployment..." && curl -fsSL https://deb.nodesource.com/setup_18.x | sudo -E bash - && echo "Node.js setup completed" && sudo apt-get install -y nodejs && echo "Node.js installed" && cd backend && echo "Changed to backend directory" && npm install && echo "npm install completed" && cd node_modules/@tropykus/tropykus-js && echo "Changed to Tropykus SDK directory" && ls -la && echo "SDK directory contents listed" && npm install && echo "SDK npm install completed" && npm run build && echo "SDK build completed" && ls -la dist/ && echo "Build output listed" && cd ../../../ && echo "Back to root directory" && python main.py
